      "pages": [
        "pagesone/collage/collage" // 拼团
      ]
    },
    {
      "root": "practicePage", // 实操练习分包
      "name": "practice",
      "pages": [
        "pagestwo/practice/practice", // 实操练习
        "pagestwo/practiceDetail/practiceDetail" // 实操练习详情
      ]
    },
    {
      "root": "answerPage", // 问答分包
      "name": "answer",
